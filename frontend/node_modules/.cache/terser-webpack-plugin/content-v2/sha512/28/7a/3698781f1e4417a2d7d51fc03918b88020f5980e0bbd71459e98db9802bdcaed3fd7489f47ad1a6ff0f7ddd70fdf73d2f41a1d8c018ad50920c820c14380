\n\n  clearAdminPost = () => {\n    this.setState({\n      title: \"\",\n      content: \"\",\n      tagsCSV: \"\",\n    });\n  };\n\n  render() {\n    if (this.state.redirect) {\n      return <Redirect to=\"/admin/login\" />;\n    } else if (this.state.loggedin) {\n      return (\n        <div className=\"container mt-5\">\n          <h1 className=\"primary\">Admin</h1>\n          <form className=\"mt-5\">\n            <div className=\"form-group row\">\n              <label htmlFor=\"inputTitle\" className=\"col-sm-2 col-form-label\">\n                Title\n              </label>\n              <div className=\"col-sm-10\">\n                <input\n                  type=\"text\"\n                  className=\"form-control\"\n                  id=\"inputTitle\"\n                  placeholder=\"Title\"\n                  autoComplete=\"off\"\n                  value={this.state.title}\n                  onChange={this.changeTitleHandler}\n                />\n              </div>\n            </div>\n            <div className=\"form-group row mt-3\">\n              <label htmlFor=\"inputTags\" className=\"col-sm-2 col-form-label\">\n                Tags (CSV)\n              </label>\n              <div className=\"col-sm-10\">\n                <input\n                  type=\"text\"\n                  className=\"form-control\"\n                  id=\"inputTitle\"\n                  placeholder=\"Tags (CSV)\"\n                  autoComplete=\"off\"\n                  value={this.state.tagsCSV}\n                  onChange={this.changeTagsHandler}\n                />\n              </div>\n            </div>\n            <div className=\"form-group mt-3\">\n              <label htmlFor=\"inputContent\">Content</label>\n              <textarea\n                className=\"form-control\"\n                id=\"inputContent\"\n                rows=\"10\"\n                value={this.state.content}\n                onChange={this.changeContentHandler}\n              ></t